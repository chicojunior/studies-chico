{
  "name": "benchmark-francisco",
  "version": "1.0.0",
  "description": "Project for host the tasks defined by the benchmark program.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "update:coverage": "npm run test:coverage && git add . && git commit -m 'coverage update'",
